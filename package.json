{
  "name": "dappy-node",
  "version": "3.20",
  "description": "Dappy main nodeJS program, stores the name records and provides a HTTP+TLS interface to interact with rnode",
  "author": "FABCO",
  "repository": "https://github.com/fabcotech/dappy-node.git",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.js",
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "start:dev:rchain": "NODE_ENV=development DAPPY_NODE_ZONE_PROVIDER=rchain node -r ts-node/register src/index.js",
    "start:dev:memory": "NODE_ENV=development DAPPY_NODE_ZONE_PROVIDER=memory node -r ts-node/register src/index.js",
    "new_name_system": "new_name_system"
  },
  "bin": {
    "new_name_system": "./scripts/new_name_system.js",
    "dappy-node": "./dist/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^6.2.3",
    "@sentry/tracing": "^6.2.3",
    "ajv": "^6.10.0",
    "blakejs": "^1.1.1",
    "body-parser": "^1.19.0",
    "dns-packet": "^5.3.1",
    "dotenv": "6.2.0",
    "elliptic": "6.5.4",
    "express": "^4.17.1",
    "rchain-token": "fabcotech/rchain-token#16.0.0",
    "rchain-toolkit": "0.4.5",
    "redis": "^4.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-spies": "^1.0.3",
    "@types/dns-packet": "^5.2.4",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.4",
    "chai-spies": "^1.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "mocha": "^9.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
