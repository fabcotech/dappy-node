DAPPY_NETWORK=dnetwork          

RCHAIN_NETWORK=mainnet
READ_ONLY=https://observer-asia.services.mainnet.rchain.coop
VALIDATOR=https://node3.root-shard.mainnet.rchain.coop,https://node1.root-shard.mainnet.rchain.coop

# used for explore-deploy and explore-deploy-x
# time (seconds) during which result are cached
# set to 0 if you don't want caching
REDIS_DB=3
CACHING=30

HTTP_PORT=3001
LAST_BLOCK_JOB_INTERVAL=40000
RCHAIN_NAMES_MASTER_REGISTRY_URI=<MASTER_URI_TO_BE_DEFINED>
RCHAIN_NAMES_CONTRACT_ID=dappynamesystem

# remove if you don't want/need sentry express monitoring
#SENTRY='https://xxx@xxx.ingest.sentry.io/xxx'

# dappy browser version and url to download the version
# leave empty to not have warning in dappy browser
DAPPY_BROWSER_MIN_VERSION=0.5.1
DAPPY_BROWSER_DOWNLOAD_LINK=https://github.com/fabcotech/dappy/releases/tag/0.5.1